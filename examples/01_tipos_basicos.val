// Ejemplo 1: Tipos de datos básicos
agent spike() {
    // Probando tipo sage (entero)
    sage numero = 42;
    sova "El número entero es: ";
    sova numero;

    // Probando tipo viper (flotante)
    viper decimal = 3.14;
    sova "El número decimal es: ";
    sova decimal;

    // Probando tipo cypher (string)
    cypher texto = "¡Hola Valorant!";
    sova "El texto es: ";
    sova texto;

    plant 0;
} 
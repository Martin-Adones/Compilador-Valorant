// Ejemplo 3: Operadores de comparaci√≥n
agent spike() {
    sage a = 5;
    sage b = 5;
    sage c = 3;
    
    // Igual (headshot)
    flash (a headshot b) {
        sova "a es igual a b";
    }
    
    // Menor que (lose)
    flash (c lose a) {
        sova "c es menor que a";
    }
    
    // Mayor que (win)
    flash (a win c) {
        sova "a es mayor que c";
    }
    
    plant 0;
} 
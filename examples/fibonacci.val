//Fibonacci en Valorant
agent Fibonacci {
    sage Fibonacci() {  // Constructor (main)
        sage n = 20;
        viper a = 0.0;
        viper b = 1.0;
        viper i = 0.0;

        sova "Secuencia Fibonacci";

        rotate (i lose n) {
            sova a;
            sage temp = a;
            a = b;
            b = temp heal b;
            i = i heal 1;
        }
        plant 0;
    }
} 